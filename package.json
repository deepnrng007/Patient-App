{
  "name": "patientmobileapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "env:qa": "ENV=qa sh envsetup.sh",
    "env:dev": "ENV=dev sh envsetup.sh",
    "env:prod": "ENV=prod sh envsetup.sh",
    "_ios": "react-native run-ios",
    "ios": "yarn env:dev && yarn _ios",
    "ios:qa": "yarn env:qa && yarn _ios",
    "ios:prod": "yarn env:prod && yarn _ios",
    "_android": "react-native run-android",
    "android": "cd android && ./gradlew clean && cd .. && npx jetify && yarn env:dev && yarn _android",
    "android:qa": "cd android && ./gradlew clean && cd .. && npx jetify && yarn env:qa && yarn _android",
    "android:prod": "cd android && ./gradlew clean && cd .. && npx jetify && yarn env:prod && yarn _android",
    "androidProdReleaseAAB": "yarn env:prod && npx jetify && cd android && ./gradlew clean && ./gradlew bundleRelease && cd .. && echo 'find file in android/app/build/outputs/bundle/release/app.aab'",
    "andridAPKRelease": "yarn env:dev && npx jetify && cd android && ./gradlew clean && ./gradlew assembleRelease",
    "linter": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'"
  },
